<Window x:Class="EquipmentGeneratorWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EquipmentGeneratorWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="522" Background="ForestGreen">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="20" VerticalAlignment="Top">
            <Button Content="Item Manager" Click="ItemManger"/>
            <Button Content="Unique" Margin="10,0,0,0" Click="Unique"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,20,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
